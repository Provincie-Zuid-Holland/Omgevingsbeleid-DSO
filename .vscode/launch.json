{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DSO: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "DSO: Attach Debug",
            "type": "debugpy",
            "request": "attach",
            "justMyCode": false,
            "connect": {
                "host": "localhost",
                "port": 5678
            },
        },
        {
            "name": "DSO generate-all",
            "type": "debugpy",
            "request": "launch",
            "module": "dso.cmds",
            "args": [
                "generate-all",
                "./input",
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "DEBUG_MODE": "True"
            }
        },
        {
            "name": "DSO generate specific",
            "type": "debugpy",
            "request": "launch",
            "module": "dso.cmds",
            "args": [
                "generate",
                "./input/omgevingsvisie/mutatie/renvooi/*SET*",
            ],
            "console": "internalConsole",
            "justMyCode": true,
            "env": {
                "DEBUG_MODE": "True"
            }
        },
    ]
}
