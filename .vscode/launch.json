{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "DSO: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": false
        },
        {
            "name": "DSO: Attach Debug",
            "type": "debugpy",
            "request": "attach",
            "justMyCode": false,
            "connect": {
                "host": "localhost",
                "port": 5678
            },
        },
        {
            "name": "DSO generate-all",
            "type": "debugpy",
            "request": "launch",
            "module": "dso.cmds",
            "args": [
                "generate-all",
                "./input",
            ],
            "console": "integratedTerminal",
            "justMyCode": true,
            "env": {
                "DEBUG_MODE": "True"
            }
        },
        {
            "name": "DSO generate specific initial",
            "type": "debugpy",
            "request": "launch",
            "module": "dso.cmds",
            "args": [
                "generate",
                "./input/01-initial",
            ],
            "console": "internalConsole",
            "justMyCode": true,
            "env": {
                "DEBUG_MODE": "True"
            }
        },
        {
            "name": "DSO generate specific mutatie",
            "type": "debugpy",
            "request": "launch",
            "module": "dso.cmds",
            "args": [
                "generate",
                "./input/02-mutation",
            ],
            "console": "internalConsole",
            "justMyCode": true,
            "env": {
                "DEBUG_MODE": "True"
            }
        },
        {
            "name": "DSO add gba",
            "type": "debugpy",
            "request": "launch",
            "module": "dso.cmds",
            "args": [
                "generate",
                "./input/03-add-gba",
            ],
            "console": "internalConsole",
            "justMyCode": true,
            "env": {
                "DEBUG_MODE": "True"
            }
        },
        {
            "name": "Run cmds.py create-area-designations",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/tools/create-value-lists/app/cmds.py",
            "args": ["create-area-designations"],
            "console": "integratedTerminal",
            "justMyCode": true,
            "cwd": "${workspaceFolder}/tools/create-value-lists"
        },
    ]
}
