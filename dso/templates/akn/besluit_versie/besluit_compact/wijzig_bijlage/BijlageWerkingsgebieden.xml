<Bijlage>
    <Kop>
        <Label>Bijlage</Label>
        <Nummer>A</Nummer>
        <Opschrift>Overzicht informatieobjecten</Opschrift>
    </Kop>
    <Divisietekst>
        <Inhoud>
            <Begrippenlijst>
                {% for werkingsgebied in werkingsgebieden %}
                <Begrip>
                    <Term>{{ werkingsgebied.Title }}</Term>
                    <Definitie>
                        <Al>
                            <ExtIoRef data-info-werkingsgebied-uuid="{{ werkingsgebied.UUID }}" ref="{{ werkingsgebied.Frbr.get_expression() }}">{{ werkingsgebied.Frbr.get_expression() }}</ExtIoRef>
                        </Al>
                    </Definitie>
                </Begrip>
                {% endfor %}
            </Begrippenlijst>
        </Inhoud>
    </Divisietekst>
</Bijlage>