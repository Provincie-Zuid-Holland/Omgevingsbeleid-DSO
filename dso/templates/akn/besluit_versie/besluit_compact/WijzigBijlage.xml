        <Label>Bijlage</Label>
        <Nummer>A</Nummer>
        <Opschrift>Bijlage bij Artikel I</Opschrift>
    </Kop>
    <RegelingVrijetekst componentnaam="{{ componentnaam }}" wordt="{{ regeling_frbr.expression }}">
