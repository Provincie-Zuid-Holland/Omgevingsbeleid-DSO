{% extends "ow/ow_base.xml" %}

{% block content %}
{% for ow_object in ow_objects %}
  <sl:stand>
    <ow-dc:owObject>
        <l:Gebiedengroep>
            <!-- {{ ow_object.object_status.value }} -->
            {% if ow_object.is_deleted() %}
            <ow:status>{{ ow_object.get_deleted_status() }}</ow:status>
            {% endif %}
            {% if ow_object.procedure_status is not none %}
            <ow:procedurestatus>{{ ow_object.procedure_status }}</ow:procedurestatus>
            {% endif %}
            <l:identificatie>{{ ow_object.identification }}</l:identificatie>
            <l:noemer>{{ ow_object.title }}</l:noemer>
            <l:groepselement>
                {% for gebied_ref in ow_object.gebieden_refs %}
                <l:GebiedRef xlink:href="{{ gebied_ref.get_href() }}"/>
                {% endfor %}
            </l:groepselement>
        </l:Gebiedengroep>
    </ow-dc:owObject>
  </sl:stand>
{% endfor %}
{% endblock %}