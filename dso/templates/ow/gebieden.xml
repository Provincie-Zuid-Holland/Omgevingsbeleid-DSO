{% extends "ow/ow_base.xml" %}

{% block content %}
{% for ow_object in ow_objects %}
  <sl:stand>
    <ow-dc:owObject>
        <l:Gebied>
            <!-- {{ ow_object.object_status.value }} -->
            {% if ow_object.is_deleted() %}
            <ow:status>{{ ow_object.get_deleted_status() }}</ow:status>
            {% endif %}
            {% if procedure_status is not none %}
            <ow:procedurestatus>{{ procedure_status }}</ow:procedurestatus>
            {% endif %}
            <l:identificatie>{{ ow_object.identification }}</l:identificatie>
            <l:noemer>{{ ow_object.title }}</l:noemer>
            <l:geometrie>
                <l:GeometrieRef xlink:href="{{ ow_object.geometry_ref }}" />
            </l:geometrie>
        </l:Gebied>
    </ow-dc:owObject>
  </sl:stand>
{% endfor %}
{% endblock %}